# Database Connection Settings
DB_TYPE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password_here
DB_NAME=your_database_name

# DBC Configuration
DBC_OUTPUT_DIR=./db_snapshots
DBC_WORKERS=10
DBC_AUTO_INSTALL=true
DBC_VERIFY_DATA=false
DBC_VERIFY_COUNTS=true

